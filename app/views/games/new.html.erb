<form action="/games" method="post">
  <label>Board Type:</label>
  <select name="board_type", id="board_type">
    <% @board_types.each do |board_type| %>
      <option value="<%= board_type %>"><%= board_type %></option>
    <% end %>
  </select>
  <label>Rows:</label>
  <input name="rows"/>
  <label>Columns:</label>
  <input name="columns"/>
  <br>
  <div class='players'>
  </div>
  <div class='add-player'>
    Add Player
  </div>
  <br>
  <input id='submit' value="Start!" type="submit" disabled/>
  <input id='player_count' value=0 name="player_count" type="hidden"/>
</form>
<hr>
